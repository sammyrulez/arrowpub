<h1>Listing shooting_sessions</h1>

<table>
  <tr>
    <th>Performed on</th>
    <th>Range</th>
    <th>Archer</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @shooting_sessions.each do |shooting_session| %>
  <tr>
    <td><%= shooting_session.performed_on %></td>
    <td><%= shooting_session.range_id %></td>
    <td><%= shooting_session.archer_id %></td>
    <td><%= link_to 'Show', shooting_session %></td>
    <td><%= link_to 'Edit', edit_shooting_session_path(shooting_session) %></td>
    <td><%= link_to 'Destroy', shooting_session, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

